<defs>
  <link rel="stylesheet" href="styles.css" />

  <symbol id="slider" type="slider" class="slider" width="0" height="0" >
    <rect width="100%" y="50%-3" height="6" fill="#505050" />
    <rect id="track" width="0" y="50%-3" height="6" fill="#e0e0e0" />
    <circle id="marker" cx="5" cy="50%" r="10" fill="#e91e63" />
    <text id="config" text-length="80" display="none" />
    <rect id="touch" width="100%" height="100%" pointer-events="visible" fill="none" />
  </symbol>
</defs>